<h1><%= @team.get('name') %></h1><a id='teams_index'>Teams</a>
<h2>Players</h2>
<table class="table">
<thead>
  <tr>
    <th>Name</th>
    <th>Number</th>
  </tr>
</thead>
<tbody>
  <% for player in @team.get('players'): %>
  <tr>
    <td><%= player.name %></td>
    <td><%= player.number %></td>
  </tr>
  <% end %>
</tbody>
</table>
